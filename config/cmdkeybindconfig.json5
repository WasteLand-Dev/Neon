{
	"enabled": true,
	// Keybinding to open the Macro editing screen
	"openMacroScreenKeybind": "key.keyboard.unknown",
	"macros": [
		{
			// Macro keybinding
			"keyName": "key.keyboard.right.control",
			// Key modifier
			"keyModName": "key.keyboard.unknown",
			// Command to execute
			"command": "/login",
			// Type of Macro. Delayed, Repeating, SingleUse, DisplayOnly
			"macroType": "SingleUse",
			// Delay(Milliseconds) used for the delay type.
			"delay": 0
		},
		{
			// Macro keybinding
			"keyName": "key.keyboard.f14",
			// Key modifier
			"keyModName": "key.keyboard.unknown",
			// Command to execute
			"command": "/sit",
			// Type of Macro. Delayed, Repeating, SingleUse, DisplayOnly
			"macroType": "SingleUse",
			// Delay(Milliseconds) used for the delay type.
			"delay": 0
		},
		{
			// Macro keybinding
			"keyName": "key.keyboard.f16",
			// Key modifier
			"keyModName": "key.keyboard.unknown",
			// Command to execute
			"command": "/lay",
			// Type of Macro. Delayed, Repeating, SingleUse, DisplayOnly
			"macroType": "SingleUse",
			// Delay(Milliseconds) used for the delay type.
			"delay": 0
		}
	]
}